# Example settings file.
# Copy this to a file named 'sourceme' and tweak as necessary.

# How to run JAVA
export JAVACMD=/usr/bin/java

# Where Saxon is installed (note: you may also need to adapt the name of the Saxon jar for the professional or home version).
export SAXONFOLDER=/usr/local/saxon
export SAXONJAR=$SAXONFOLDER/saxon9ee.jar

# If our Saxon is an EE version we can compile the Saxon-JS XSLT stylesheet without Oxygen
export SAXON_EE_AVAILABLE=true
